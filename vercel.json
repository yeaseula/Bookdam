{
"headers": [
    {
    "source": "/(.*\\.(js|css|woff2|png|jpg|svg))",
    "headers": [
        {
        "key": "Cache-Control",
        "value": "public, max-age=31536000, immutable"
        }
    ]
    },
    {
    "source": "/(.*\\.html)",
    "headers": [
        {
        "key": "Cache-Control",
        "value": "no-cache, no-store, must-revalidate"
        }
    ]
    }
]
}
